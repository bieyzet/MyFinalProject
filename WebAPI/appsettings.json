{
  "TokenOptions": {
    "Audience": "engin@engin.com",//kitle,proje web adresi
    "Issuer": "engin@engin.com",//audience'yi uygulayan
    "AccessTokenExpiration": 10,//token gecerlilik süresi(dakika)
    "SecurityKey": "mysupersecretkeymysupersecretkey"//anahtar ismi,keyimiz sistemi koruma amaclý
  },//Register Claims,anahtar olusturduk
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    } //loglama configurasyonu
  },
  "AllowedHosts": "*"
}
//tokenOpt. ve log birer section(bölüm)